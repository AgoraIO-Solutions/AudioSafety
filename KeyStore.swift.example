import Foundation

struct KeyStore {
    /// Your Agora App ID from https://console.agora.io/
    static let appId: String = "YOUR_AGORA_APP_ID"
    
    /// Optional: Your Agora temporary token (leave empty for testing without token)
    /// Generate tokens at https://console.agora.io/
    static let token: String? = nil
    
    /// The channel name for testing (can be any string)
    static let channelName: String = "test-channel"
}
